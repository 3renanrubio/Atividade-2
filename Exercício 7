7- #include <stdio.h>

int soma_vetor(int a, int b, int c);

int main() {

int num1, num2, num3, resultado;

printf("Digite o primeiro número: ");

scanf("%d", &num1);

printf("Digite o segundo número: ");

scanf("%d", &num2);

printf("Digite o terceiro número: ");

scanf("%d", &num3);

resultado = soma_vetor(num1, num2, num3);

printf ("A soma dos três é: %d\n", resultado);

return 0;

}
